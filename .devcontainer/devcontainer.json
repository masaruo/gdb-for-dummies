{
    "name": "C/C++ Development",
    "dockerComposeFile": "docker-compose.yaml",
    "service": "cpp_dev",
    "workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools",
                "ms-vscode.cpptools-themes",
                "ms-vscode.cpptools-extension-pack"
            ]
        }
    },
    "remoteUser": "vscode",
    "updateRemoteUserUID": true,
    "shutdownAction": "stopCompose"
}
